{
  "server": {
    "port": 3000
  },
  "baseHref": {
    "development": "/",
    "production": "/"
  },
  "jsFileName": {
    "development": "js/[name].[hash].js",
    "production": "js/[name].[chunkhash].js"
  },
  "global": [
    {
      "name": "self",
      "path": {
        "fake": "//localhost:3001/api/",
        "development": "/api",
        "production": "/api"
      }
    },
    {
      "name": "assetsPath",
      "path": {
        "fake": "assets",
        "development": "assets",
        "production": "assets"
      }
    }
  ],
  "buildFolder": "build"
}
